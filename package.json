{
  "name": "graphql-api-base",
  "version": "1.0.0",
  "description": "A GraphQL API base",
  "main": "./dist/main.js",
  "author": "Jeremy Shimko <jeremy.shimko@gmail.com>",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jshimko/graphql-api-base.git"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "rimraf dist && babel src -d dist --copy-files --quiet",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "start": "docker-compose up -d mongo redis && nodemon --watch src --ext js,graphql --exec 'npm run build && LOG_LEVEL=debug node dist/main.js'",
    "start:prod": "nodemon --watch src --ext js,graphql --exec 'npm run build && LOG_LEVEL=debug node dist/main.js'",
    "serve": "NODE_ENV=production node dist/main.js"
  },
  "dependencies": {
    "@google-cloud/storage": "2.0.3",
    "@prinzdezibel/graphql-to-mongodb": "1.3.6",
    "@sendgrid/mail": "6.3.1",
    "apollo-server-express": "2.0.6",
    "babel-plugin-transform-runtime": "6.23.0",
    "bcrypt": "3.0.0",
    "bull": "3.4.8",
    "bunyan": "2.0.2",
    "bunyan-format": "0.2.1",
    "dataloader": "1.4.0",
    "date-fns": "^1.29.0",
    "express": "4.16.3",
    "graphql": "14.0.2",
    "graphql-iso-date": "3.5.0",
    "graphql-playground-middleware-express": "1.7.3",
    "graphql-server-express": "1.4.0",
    "graphql-subscriptions": "0.5.8",
    "graphql-tools": "3.1.1",
    "handlebars": "4.0.12",
    "html-to-text": "4.0.0",
    "jwt-simple": "0.5.1",
    "lodash": "4.17.11",
    "mongodb": "3.1.4",
    "node-loggly-bulk": "2.2.4",
    "nodeify": "1.0.1",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "pluralize": "7.0.0",
    "slugify": "1.3.1",
    "subscriptions-transport-ws": "0.9.14",
    "url": "0.11.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "9.0.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-2": "6.24.1",
    "dotenv": "6.0.0",
    "eslint": "5.5.0",
    "eslint-plugin-babel": "5.2.0",
    "mongodb-prebuilt": "6.3.6",
    "nodemon": "1.18.4",
    "rimraf": "2.6.2"
  }
}
